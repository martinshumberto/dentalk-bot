{
  "name": "dentalk-bot",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "dev:client": "cd ./client && export NODE_ENV=development && vue-cli-service serve",
    "dev:server": "cd ./server && export NODE_ENV=development && nodemon --exec babel-node ./src/app.js --debug",
    "build:client": "cd ./client && vue-cli-service build",
    "build:server": "cd ./server && webpack",
    "lint:client": "cd ./client && vue-cli-service lint",
    "lint-fix:client": "cd ./client && vue-cli-service lint --fix",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "clean": "rm -rf build && mkdir build && rm -rf public && mkdir public",
    "sequelize": "cd ./server && export NODE_ENV=development && sequelize $*",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "watch": "webpack --progress --colors --watch",
    "start": "node ./build/main.bundle.js",
    "debug": "node --debug build/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "messenger",
    "bot"
  ],
  "author": "Humberto Martins",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "actions-on-google": "^2.12.0",
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.0.0",
    "chartist": "^0.11.0",
    "comtele-sdk": "^1.1.6",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dialogflow": "^1.2.0",
    "dotenv": "^8.2.0",
    "es6-promise": "^4.2.4",
    "express": "^4.17.1",
    "googleapis": "^39.2.0",
    "kalendar-vue": "^2.1.1",
    "moment": "^2.26.0",
    "mysql2": "^2.1.0",
    "pb-util": "^0.1.3",
    "portal-vue": "^2.1.7",
    "register-service-worker": "^1.7.1",
    "request": "^2.88.2",
    "sequelize": "^5.21.11",
    "uuid": "^7.0.2",
    "vue": "^2.6.10",
    "vue-clickaway": "^2.1.0",
    "vue-notifyjs": "^0.3.0",
    "vue-router": "^3.3.2",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
